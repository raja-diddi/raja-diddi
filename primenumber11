#include<iostream>
using namespace std;
long long int x=1000001;
long long int seive[1000001];
void gen_seive()
{
		for(int i=0;i<x;i++)
	seive[i]=1;
	seive[0]=seive[1]=0;		
	for(int i=2;i*i<x;i++)
	{
		if(seive[i]==1)			
		{					
			for(int j=i*i;j<=x;j=j+i)
				seive[j]=0;
		}
	}
}
int main()
{
	gen_seiver();	
	int n,lv,rv,i,s=0;
	cin>>n;
  while(n--)
  {
    cin>>lv>>rv;
    s=0;
    for(i=lv;i<=rv;i++)
    {
      if(seive[i]==1)
        s=s+i;
    }
    cout<<sum<<endl;
  }
  
	return 0;
}
