*/

long long int x=1000000;
long long int seive[1000000];
void gen-seiver()
{
	for(int i=0;i<x;i++)  
	seive[i]=1;
	seive[0]=seive[1]=0;		
	for(int i=2;i*i<x;i++)
	{
		if(seive[i]==1)				
		{					
			for(int j=i*i;j<=x;j=j+i)
				seive[j]=0;
		}
	}
}
int main()
{
	gen-seive();		
	int q;
	cin>>q;
    while(q--)
    {
    int n;
    cin>>n;
     for(int i=2;i<=n;i++)
     {
       if(seive[i]==1)
       cout<<seive[i]<<" ";
      }
      }
	return 0;
}
